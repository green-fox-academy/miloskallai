<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8" />
  <title>Cocktails</title>
  <link rel="stylesheet" type="text/css" href="css/style.css" />
  <script src="main.js"></script>
</head>

<body>
  <h1>Cocktails</h1>
<div class="cocktail-list">
  <a>ALL</a>
  <% alcoholList.forEach(alcohol => { %>
    <a><%= alcohol.toUpperCase() %></a>
 <% }) %>
</div>

  <table>
    <tr>
      <th>Name</th>
      <th>Price</th>
    </tr>
    <tr>
<% cocktails.forEach(cocktail => { %>
  <tr>
    <td><p><%= cocktail.name %></p><p><%= cocktail.contains %></p></td>
    <td><%=cocktail.price  %></td>
  </tr>
<% }) %>
</tr>
  </table>

</body>

</html>